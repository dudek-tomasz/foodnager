---
// Prosty healthcheck bez zależności od Supabase
const env = {
  hasSupabaseUrl: !!import.meta.env.SUPABASE_URL,
  hasSupabaseKey: !!import.meta.env.SUPABASE_KEY,
  nodeEnv: import.meta.env.MODE,
};
---

<!DOCTYPE html>
<html>
